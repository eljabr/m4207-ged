{% extends 'base.html.twig' %}

{% block title %}Hello AdminController!{% endblock %}

{% block body %}
  <h1>Dashboard</h1>

<table class="table table-hover">
  <thead>
    <tr>
      <th scope="col">Id </th>
      <th scope="col">Nom</th>
      <th scope="col">Genre</th>
      <th scope="col">Autorisation</th>
      <th scope="col">Actions</th>
    </tr>
  </thead>
  <tbody>
{% for item in listeDocuments %}
<tr class="table-active">
      <th scope="row"> {{ item.document.id }} </th>
      <td>{{ item.document.nom }}</td>
      <td>{{ item.document.type.type }}</td>
      <td>{{ item.autorisation.id }}</td>
      <td>liste</td>
    </tr>
{% endfor %}
   </tbody>
</table>
{% endblock %}